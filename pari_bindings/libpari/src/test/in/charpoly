test(p)=
{
  my(P,Q);
  P=random(x^10*'b^10*Mod(1,p))*Mod(1,ffinit(p,10,'b));
  Q=random(x^10*'b^10*Mod(1,p))*Mod(1,ffinit(p,10,'b));
  my(z=Mod(P,Q));
  if(subst(minpoly(z),x,z)!=0,error(z));
  if(subst(charpoly(z),x,z)!=0,error(z));
}
test(5);
test(nextprime(2^40));
test(nextprime(2^100));

