{
L = [[5,1111], [5,7523], [5,36227], [5,36293], [5,36322], [5,42853], [5,-5657], [5,-14606], [3,853], [3,9546], [3,11703], [3,13767], [3,13894], [3,15338]];
for (i = 1, #L,
  [p,m] = L[i];
  f = abs( if (m%4 == 1, 5*m, 20*m) );
  H = [a | a<-[1..f], a % 5 == 1 && gcd(a,f) == 1 && kronecker(m,a) == 1];
  print(subcyclopclgp([f,H],p))
);
}

