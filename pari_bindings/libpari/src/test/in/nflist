     [[6,8],6],[[6,9],5.5],[[6,10],6],[[6,11],5],[[6,13],4.5],
  ["C7",15],["D7",6],["M21",9],["M42",6],
  ["C9",12],[[9,2],12],["D9",9],
  ["C11",17],["D11",10]
];}

for(i=1,#vG,my([GP,lim]=vG[i]); do(GP,lim));
