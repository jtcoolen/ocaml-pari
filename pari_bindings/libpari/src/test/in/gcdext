chkgcdext(a,b,p)=
{
  my(P=random(Mod(1,p)*x^a),Q=random(Mod(1,p)*x^b));
  my([u,v,d]=gcdext(P,Q));
  if(u*P+v*Q!=d,error([P,Q,p]));
  my([u,v,d]=polresultantext(P,Q));
  if(u*P+v*Q!=d,error([P,Q,p]));
}

chkgcdext(200,200,17)
chkgcdext(200,200,nextprime(2^31))
chkgcdext(200,200,nextprime(2^63))
chkgcdext(200,200,nextprime(2^95))
chkgcdext(300,100,17)
chkgcdext(300,100,nextprime(2^31))
chkgcdext(300,100,nextprime(2^63))
chkgcdext(300,100,nextprime(2^95))
chkgcdext(200,300,nextprime(2^31))
chkgcdext(200,300,nextprime(2^63))
chkgcdext(200,300,nextprime(2^95))

chkhalfgcd(a,b)=
{
  my([M,V]=halfgcd(a,b));
  if (M*[a,b]~!=V, error([a,b]));
  M*[a,b]~-V;
}

chkhalfgcd(858824,528747)
