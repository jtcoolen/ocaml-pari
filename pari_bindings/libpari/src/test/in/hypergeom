default(parisize,"10M") \\ 8M overflows
testF(v, D0, D) =
{
  for(i=1,#v,
    my ([n,d,z] = v[i]);
    localbitprec(D);
    a = hypergeom(n,d,z);
    localbitprec(D + 64);
    b = hypergeom(n,d,z);
    e = exponent(a/b - 1);
    if (e > -D0, print(i,": ", e));
  );
}
