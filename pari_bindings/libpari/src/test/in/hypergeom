\\ #2382
f(a, b, c, r, t) =
{
  A = round(10*a)/10;
  B = round(10*b)/10;
  C = round(10*c)/10;
  R = round(10*r)/10;
  T = round(10*t)/10;
  z = 1 - r*exp(t*I*Pi);
  Z = 1 - R*exp(T*I*Pi);
  exponent(hypergeom([a,b],[c],z) - hypergeom([A,B],[C],Z));
}
f(1.1, 1.3, 1.4, 0.8, -0.3)
f(1.1, 0.3, 3.4, 0.1, -0.4)
f(1.1, 0.3, 4.4, -0.3, 0.8)
f(1.2, 0.3, 3.5, 0.2, -0.4)
f(1.2, 0.3, 4.5, -0.3, 0.8)
f(1.2, 1.2, 1.4, -0.3, 0.7)

